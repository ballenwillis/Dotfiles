
# git
alias g='git'
alias ga='git add -A'
alias gb='git branch'
alias gbd='git branch -D'
alias gbr='git ls-remote --heads'
alias gcam='git commit -am'
alias gcm='git commit -m'
alias gco='git checkout'
alias gcob='git checkout -b' 
alias gdif='git diff'
alias gfm='git pull'
alias gfr='git pull --rebase'
alias gm='git merge'
alias gp='git push'
alias gpu='git push -u origin'
alias grh='git reset --hard'
alias gs='git stash' 
alias gsa='git stash apply'
alias gst='git status'

# yarn
alias y='yarn'
alias ys='yarn start'
alias yys='yarn && yarn start'
alias ya='yarn add'

# Own The Spot
alias ots='cd ~/Desktop/OTS/Own-The-Spot'
alias run-ots='teamocil --here ots'
alias otsf='ots && cd Own-The-Spot'
alias otsdb='ots && cd Database'
alias otsapi='ots && cd API'

alias run-otsf='otsf && yys'
alias run-otsdb='otsdb && yys'
alias run-otsapi='otsapi && node app.js'

alias ots-dev-db='echo "Not sure what I should do here"'
alias ots-prod-db='echo "Not sure what I should do here"'

